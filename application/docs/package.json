{
  "name": "docs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "cf:build": "opennextjs-cloudflare build",
    "cf:deploy": "opennextjs-cloudflare deploy",
    "cf:gen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "cf:preview": "opennextjs-cloudflare preview",
    "cf:upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "db:gen": "drizzle-kit generate",
    "db:local": "wrangler d1 migrations apply replacewithyourown --local",
    "db:remote": "wrangler d1 migrations apply replacewithyourown --remote",
    "db:studio": "drizzle-kit studio",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1",
    "@shapelesss/core": "^1.0.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@tanstack/react-query": "^5.51.23",
    "nanoid": "^5.1.5",
    "better-sqlite3": "^12.2.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.39.0",
    "hono": "^4.7.0",
    "next": "^15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.5.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250214.0",
    "@types/better-sqlite3": "^7",
    "@types/node": "^22.10.6",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.30.1",
    "eslint": "^9.18.0",
    "eslint-config-next": "^15.1.4",
    "eslint-plugin-drizzle": "^0.2.3",
    "postcss": "^8",
    "tailwindcss": "^4.0.0",
    "typescript": "^5",
    "wrangler": "^4.0.0"
  }
}